server:
  port: 45291

spring:
  application:
    name: profile-service

  datasource:
    url: jdbc:postgresql://profileDb:5432/profileDb
    username: postgres
    password: postgres
    driver-class-name: org.postgresql.Driver

  jpa:
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
    hibernate:
      ddl-auto: update

  cloud:
    consul:
      host: http://consul
      port: 8500